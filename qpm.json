{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "custom-types",
    "id": "custom-types",
    "version": "0.18.0",
    "url": "https://github.com/QuestPackageManager/Il2CppQuestTypePatching",
    "additionalData": {
      "overrideSoName": "libcustom-types.so",
      "compileOptions": {
        "cppFlags": [
          "-Wno-invalid-offsetof"
        ]
      },
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./build.ps1"
      ],
      "qmod": [
        "pwsh ./build.ps1 -clean",
        "qpm qmod manifest",
        "pwsh ./createqmod.ps1 CustomTypes -clean"
      ],
      "test": [
        "pwsh ./build.ps1 -local_test"
      ]
    },
    "ndk": "^27.3.13750724",
    "qmodIncludeDirs": [
      "./extern/libs",
      "./build"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "CustomTypes.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.2.0",
      "additionalData": {}
    },
    {
      "id": "capstone",
      "versionRange": "^0.1.0",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.2.1",
      "additionalData": {}
    }
  ]
}